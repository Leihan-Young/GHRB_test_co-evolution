        "url": "https://github.com/JodaOrg/joda-time/pull/572",
        "resourcePath": "/JodaOrg/joda-time/pull/572",
        "createdAt": "2021-10-25T07:35:11Z",
        "number": 572,
        "mergeCommit": null,
        "potentialMergeCommit": null,
        "files": {
          "edges": [
            {
              "node": {
                "changeType": "MODIFIED",
                "path": "src/main/java/org/joda/time/tz/src/europe"
              }
            },
            {
              "node": {
                "changeType": "MODIFIED",
                "path": "src/site/xdoc/timezones.xml"
              }
            }
          ]
        },
        "commits": {
          "edges": [
            {
              "node": {
                "id": "PURC_lADOABrMvs4tm5ba2gAoNzhkMTlhNjAyOGUxZTMyZjNkMmM3MGZiMjIxZGRjMjA1YmExNzdlMQ",
